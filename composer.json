{
  "name": "gocardless/pro-client-php",
  "description": "GoCardless Pro PHP Client Library",
  "keywords": [
    "gocardless",
    "direct debit",
    "api"
  ],
  "homepage": "https://gocardless.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Gocardless and contributors",
      "homepage": "https://github.com/gocardless/pro-client-php/contributors"
    }
  ],
  "require": {
    "php": ">=5.2.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "~0.6.1",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "version": "0.0.1",
  "autoload": {
    "psr-4": { 
      "GoCardless\\" : ["lib/", "tests/"]
    }
  }
}
